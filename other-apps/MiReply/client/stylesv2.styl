#filter_clusters_form {
	display: none;
}

#filter_clusters_form input {
	display: none;
	width: 300px;
	height: 27px;
	border-radius: 3px;
	box-shadow: 0px 1px 3px rgba(0,0,0,.5) inset;
	border: 1px solid rgba(0,0,0,.4);
	font-size: .8em;
	padding-left: 10px;	
}

.cluster_header {
	display: none;
	width: 550px;
	margin: 0 auto 25px auto;
}

.cluster_header button {
	display: none;
	height: 50px;
	width: 450px;
	border: none;
	background-color: rgba(224,71,71,.75);
	color: white;
	border-radius: 0px;
	font-size: 1.2em;
	font-weight: 300;
	font-style: none;
	margin-left: 52px;
}

.cluster_header button:hover {
	background-color: rgba(224,71,71,.65);
	cursor: pointer;
}

.cluster_card_3d_container {
	width: 229px;
	height: 229px;
	position: relative;
	perspective: 650px;
	display: inline-block;
	margin: 1px 2.5px;
}

.cluster_card {
	width: 100%;
	height: 100%;
	position: absolute;
	transform-style: preserve-3d;
	transition: transform .3s;
}

.cluster_card figure {
  margin: 0;
  display: block;
  position: absolute;
  width: 100%;
  height: 100%;
  backface-visibility: hidden;
}

.cluster_card .front {
	cursor: pointer;
}

.cluster_card .back h2 {
	font-size: 1.6em;
	text-align: center;
	width: 150px;
	margin 60px auto;
}
.cluster_card .back {
	cursor: pointer;
	background-color: headerBrown;
	transform: rotateY( 180deg );
}

.flipped {
  transform: rotateY( 180deg );
}

.flipped-up {
  transform: rotateX( 180deg );
}

.flip_icon {
	cursor: pointer;
}

.flip_icon_close {
	cursor: pointer;
}

.cluster_card .contained {
	color: white;
	padding: 10px;
}

.cluster_card .front .contained h3 {
	display: block;
	margin: 15px 0px 0px 15px;
	padding-bottom: 2px;
	font-size: 1.7em;
	overflow-x: hidden;
	font-weight: 400;
	cursor: pointer;
}

.card_footer {
	z-index: 10000;
	position: absolute;
	bottom: 0px;
	left: 0px;
	height: 60px;
	background-color: headerBrown;
	width: 229px;
}

.card_footer img {
	width: 52px;
	position: absolute;
	left: 90px;
	top: 4px;
}

.cluster_card .front .contained ul {
	margin: 12px auto;
	width: 150px;
	text-align: center;
	font-size: 1.1em;
	line-height: 20px;
}

.cluster_card .front .contained h4 {
	margin: 10px 0px 0px 15px;
	float: left;
	font-size: .7em;
	color: rgba(0,0,0,.75);
}

.cluster_card .back .contained div img {
	width: 35px;

}

.cluster_card .back .contained div {
	margin-top: 15px;
	text-align: center;
	width: 130px;
	margin: 17px 0px -5px 90px;
	background-color: rgba(0,0,0,.1);
	border-radius: 5px;
}

.cluster_card .back .contained div:hover {
	background-color: rgba(0,0,0,.3);
	cursor: pointer
}

.cluster_card .back .contained div:hover h3 {
	color: white;

}

.cluster_card .back .contained h3 {
	display: inline-block;
	vertical-align: top;
	margin-top: 10px;
}

.cluster_card .back .contained span {
	padding:10px 5px 0px;
	background-color: rgba(200,10,10,.7);
	border-radius: 5px;
	color: white;
	cursor: pointer;
}

.cluster_card .back .contained span h4 {
	font-size: .8em;
	position: relative;
	top: 3px;
	display: inline-block;
	vertical-align: top;
}

.cluster_card .back .contained span img {
	width:17px;
}

.cluster_card .back .contained > img {
	width: 17px;
	float: right;
}

